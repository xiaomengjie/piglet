<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_word"
    app:startDestination="@id/total_word_fragment">

    <fragment android:id="@+id/total_word_fragment"
        android:name="com.example.www.saveps.ui.word.TotalWordFragment"
        tools:layout="@layout/fragment_total_word">
        <action
            android:id="@+id/action_total_word_fragment_to_search_word_fragment"
            app:destination="@id/search_word_fragment" />
        <action
            android:id="@+id/action_total_word_fragment_to_word_detail_fragment"
            app:destination="@id/word_detail_fragment" />
    </fragment>

    <fragment
        android:id="@+id/search_word_fragment"
        android:name="com.example.www.saveps.ui.word.SearchWordFragment"
        tools:layout="@layout/fragment_search_word" />
    <fragment
        android:id="@+id/word_detail_fragment"
        android:name="com.example.www.saveps.ui.word.WordDetailFragment"
        tools:layout="@layout/fragment_word_detail" >
        <argument
            android:name="arguments_english_word"
            app:argType="string" />
    </fragment>
</navigation>